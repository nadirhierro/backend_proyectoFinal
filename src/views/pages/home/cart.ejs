<html lang="en">
  <head>
    <%- include("../../partials/head"); %>
  </head>
  <body>
    <header><%- include("../../partials/headerUsers"); %></header>
    <main class="main">
      <%- include("../../partials/hero"); %> <% if (cartId) { %>
      <div class="container-fluid cart">
        <div class="row justify-content-center tituloFinalizar">
          <h2 class="col-12 text-center">¡Últimos pasos!</h2>
          <h4 class="col-12 text-center">Chequeá tu carrito...</h4>
        </div>
        <div class="row justify-content-center carritoYdatos">
          <%- include("../../partials/cart/cartTable",{ cartId: cartId,
          products:products}); %> <% } else { %>
          <h2>No abriste el carrito!</h2>
          <a class="btn btn-primary" href="/products">Ir a tienda</a>
          <% } %>
        </div>
      </div>
    </main>
    <script src="/assets/js/cart.js"></script>
  </body>
</html>
